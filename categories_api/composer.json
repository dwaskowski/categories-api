{
    "name": "dwaskowski/categories-api",
    "description": "Categories API",
    "license": "MIT",
    "autoload": {
        "classmap": [
            "propel/generated-classes/"
        ],
        "psr-4": {
            "Api\\": "src/Api",
            "Application\\": "src/Application"
        }
    },
    "authors": [
        {
            "name": "Dmitrij Waskowski",
            "email": "dymitr@gmail.com",
            "homepage": "http://www.dimidoff.info/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "martynbiz/slim3-controller": "~0.0.0",
        "monolog/monolog": "^1.17",
        "propel/propel": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "codeception/codeception": "~2.2"
    },
    "autoload-dev": {
        "psr-4": {
            "PhpUnit\\": "tests/unit/",
            "Integration\\": "tests/integration/"
        }
    }
}
